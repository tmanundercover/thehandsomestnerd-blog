{
  "name": "thehandsomestnerd-blog",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@sanity/block-content-to-react": "^3.0.0",
    "@sanity/client": "^2.8.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/aos": "^3.0.4",
    "@types/material-ui": "^0.21.8",
    "@types/react-router-dom": "^5.3.2",
    "aos": "^2.3.4",
    "axios": "^0.21.1",
    "framer-motion": "^4.1.17",
    "material-ui-popup-state": "^1.8.4",
    "moment": "^2.29.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-material-ui-carousel": "^2.2.7",
    "react-moment": "^1.1.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-syntax-highlighter": "^15.4.3",
    "ts-node": "^10.1.0",
    "typescript": "^4.2.4",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "NODE_ENV=production react-scripts start",
    "start:debug": "yarn build:dev && NODE_ENV=development serve -s build",
    "start:dev": "NODE_ENV=development react-scripts start",
    "build": "tsc && react-scripts build",
    "build:dev": "tsc --build && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "firebaseDeploy": "firebase deploy --only thehandsomestnerd",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@sanity/image-url": "^0.140.22",
    "@storybook/addon-actions": "^6.4.9",
    "@storybook/addon-essentials": "^6.4.9",
    "@storybook/addon-links": "^6.4.9",
    "@storybook/node-logger": "^6.4.9",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.4.9",
    "@types/react": "^17.0.38",
    "@types/react-syntax-highlighter": "^13.5.0"
  }
}
